<div class="inner">
   <div class="logo">
      DuelingPets.net
   </div>
</div>
<div class="inner">
   <div class="userbar">
      <% if signed_in? %>
         <%#= link_to "Users", '#' %>
         <% if current_user.admin? %>
            $<%= link_to current_user.vname, current_user %>
         <% else %>
            ~<%= link_to current_user.vname, current_user %>
         <% end %>
         <%= link_to "Log out", logout_path, method: "delete" %>
      <% else %>
         <%= link_to "Sign in", signin_path %>
         <%= link_to "Sign up", signup_path %>
      <% end %>
   </div>
</div>
