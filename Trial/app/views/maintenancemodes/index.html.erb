<h1>Listing maintenancemodes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Created on</th>
    <th>Maintenance on</th>
    <th></th>
  </tr>

<% @maintenancemodes.each do |maintenancemode| %>
  <tr>
    <td><%= maintenancemode.name %></td>
    <td><%= distance_of_time_in_words_to_now maintenancemode.created_on %> ago&nbsp;&nbsp;</td>
    <td><%= maintenancemode.maintenance_on %></td>
    <td><%= button_to 'Edit', edit_maintenancemode_path(maintenancemode), method: :get %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Maintenancemode', new_maintenancemode_path %>
