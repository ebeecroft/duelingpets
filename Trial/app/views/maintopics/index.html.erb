<h1>Listing maintopics</h1>

<table>
  <tr>
    <th>Id</th>
    <th>User</th>
    <th>Topicname</th>
    <th>Created on</th>
    <th>Maintenance</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @maintopics.each do |maintopic| %>
  <tr>
    <td><%= maintopic.id %></td>
    <td><%= maintopic.user_id %></td>
    <td><%= maintopic.topicname %></td>
    <td><%= maintopic.created_on %></td>
    <td><%= maintopic.maintenance %></td>
    <td><%= link_to 'Show', maintopic %></td>
    <td><%= link_to 'Edit', edit_maintopic_path(maintopic) %></td>
    <td><%= link_to 'Destroy', maintopic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Maintopic', new_maintopic_path %>
